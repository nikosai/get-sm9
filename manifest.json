{
  "name": "右クリックでURL最終部をコピー",
  "description": "An extension to copy the last part of URL of the page.",
  "version": "0.2",
  "manifest_version": 2,
  "permissions": [
    "contextMenus",
    "clipboardWrite",
    "activeTab"
  ],
  "icons": {
    "128": "128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  }
}